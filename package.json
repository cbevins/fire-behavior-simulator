{
  "name": "@cbevins/fire-behavior-simulator",
  "version": "0.4.0",
  "description": "A standalone bundle for performing BehavePlus v6 fire simulations in Node or web browsers",
  "author": "Collin D. Bevins <cbevins@montana.com>",
  "license": "MIT",
  "keywords": [
    "fire behavior",
    "wildland fire behavior",
    "BehavePlus"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbevins/fire-behavior-simulator.git"
  },
  "bugs": {
    "url": "https://github.com/cbevins/fire-behavior-simulator/issues"
  },
  "homepage": "https://github.com/cbevins/fire-behavior-simulator/readme",
  "main": "./dist/bundle.cjs.js",
  "exports": "./dist/bundle.esm.js",
  "module": "./dist/bundle.esm.js",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "RELEASE_NOTES.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "rollup -c",
    "compiler": "node ./src/fire-behavior-genome/compiler/GenomeCompiler.js",
    "fuelsLister": "node ./utils/fuelsLister.js",
    "lint": "eslint \"**/*.js\" -o eslint.output",
    "lint:fix": "prettier-eslint \"**/*.js\" --write",
    "variablesLister": "node ./utils/variablesLister.js",
    "prebuild": "rimraf dist",
    "publish:github": "npm publish --registry https://npm.pkg.github.com --access public",
    "publish:npm": "npm publish --registry https://registry.npmjs.org --access public",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "test:coverage": "jest --coverage",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "test:watch": "jest --watchAll"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "publishConfigGithub": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "publishConfigNpm": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/src/untracked"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/index.js",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/untracked/**",
      "!**/utils/**",
      "!**/node_modules/**",
      "!**/.eslintrc.js",
      "!rollup.config.js",
      "!**/dag/__behave_tests__/**",
      "!**/dev/**",
      "!**/docs/**",
      "!**/examples/**",
      "!**/fire-behavior-models/Contain.js",
      "!**/fire-behavior-models/Dag.js",
      "!**/fire-behavior-genome/compiler/**",
      "!**/mega.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-json": "~4",
    "@rollup/plugin-node-resolve": "~9",
    "@rollup/pluginutils": "4.1.0",
    "coveralls": "~3",
    "eslint": "7.22.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.2.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-standard": "4.1.0",
    "jest": "26.6.3",
    "papaparse": "5.3.0",
    "rimraf": "~3",
    "rollup": "2.41.2",
    "rollup-plugin-terser": "~7",
    "rollup-plugin-uglify": "~6"
  }
}
